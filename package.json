{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "start": "concurrently npm:start:*",
    "build": "concurrently -r npm:build:*",
    "wait": "concurrently -r npm:wait:*",
    "open": "OPEN_MATCH_HOST_ONLY=true; node -e 'require(\"better-opn\")(\"http://localhost:3000\")'",
    "start:remix": "npm run wait && remix watch",
    "start:express": "npm run wait && cross-env NODE_ENV=development node-dev --notify=false server/index.js --ignore server/build",
    "start:docker": "docker-compose up",
    "start:open": "wait-on tcp:3000 && npm run open",
    "start:css": "tailwindcss -i ./app/main.css -o ./app/tailwind.css --watch",
    "build:remix": "remix build",
    "prebuild:remix": "tailwindcss -i ./app/main.css -o ./app/tailwind.css -m",
    "postinstall": "remix setup node",
    "wait:redis": "wait-on tcp:6379"
  },
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "@remix-run/express": "1.5.1",
    "@remix-run/react": "1.5.1",
    "bootstrap-icons": "1.8.2",
    "clsx": "1.1.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "date-fns": "2.28.0",
    "escape-html": "1.0.3",
    "express": "4.18.1",
    "front-matter": "4.0.2",
    "hast-util-to-string": "2.0.0",
    "http-proxy-middleware": "2.0.6",
    "ioredis": "5.0.5",
    "lru-cache": "7.10.1",
    "morgan": "1.10.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "rehype-document": "6.0.1",
    "rehype-format": "4.0.1",
    "rehype-parse": "8.0.4",
    "rehype-stringify": "9.0.3",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remix": "1.5.1",
    "sharp": "0.30.4",
    "unified": "10.1.2",
    "zod": "3.17.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.5.1",
    "@types/escape-html": "1.0.2",
    "@types/ioredis": "4.28.10",
    "@types/lru-cache": "7.10.9",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/sharp": "0.30.2",
    "@types/tailwindcss": "3.0.10",
    "better-opn": "3.0.2",
    "color-convert": "2.0.1",
    "concurrently": "7.2.1",
    "node-dev": "7.4.3",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "wait-on": "6.0.1"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
