{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "start": "concurrently npm:start:*",
    "build": "remix build",
    "wait": "concurrently -r npm:wait:*",
    "open": "OPEN_MATCH_HOST_ONLY=true; node -e 'require(\"better-opn\")(\"http://localhost:3000\")'",
    "start:remix": "npm run wait && remix watch",
    "start:express": "npm run wait && cross-env NODE_ENV=development node-dev --notify=false server/index.js --ignore server/build/index.js",
    "start:docker": "docker-compose up",
    "start:open": "wait-on tcp:3000 && npm run open",
    "postinstall": "remix setup node",
    "wait:redis": "wait-on tcp:6379"
  },
  "dependencies": {
    "@remix-run/express": "1.1.3",
    "@remix-run/react": "1.1.3",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "date-fns": "2.28.0",
    "express": "4.17.1",
    "hast-util-to-string": "2.0.0",
    "http-proxy-middleware": "2.0.2",
    "ioredis": "4.28.3",
    "lru-cache": "6.0.0",
    "morgan": "1.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-document": "6.0.1",
    "rehype-format": "4.0.1",
    "rehype-parse": "8.0.4",
    "rehype-stringify": "9.0.3",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remix": "1.1.3",
    "unified": "10.1.1",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "1.1.3",
    "@types/ioredis": "4.28.7",
    "@types/lru-cache": "5.1.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "better-opn": "3.0.2",
    "concurrently": "7.0.0",
    "node-dev": "7.1.0",
    "typescript": "4.1.2",
    "wait-on": "6.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
